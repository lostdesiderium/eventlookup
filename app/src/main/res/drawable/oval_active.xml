<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval"
    >

    <size
        android:height="10dp"
        android:width="10dp" />

    <corners
        android:radius="10dp" />

    <gradient
        android:startColor="@color/colorAccentSoftRed"
        android:endColor="@color/colorDarkWhite"
        android:angle="45"
        android:type="linear"
        />

    <stroke android:color="@color/colorDarkWhite"
        android:width="1dp" />


</shape>